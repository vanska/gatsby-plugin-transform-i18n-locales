{
  "name": "gatsby-plugin-transform-i18n-locales",
  "description": "Transform i18n locale files in to gatsby nodes",
  "version": "1.0.2",
  "author": "Mikko Vänskä <mikkovanska.com@gmail.com>",
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "cross-env": "^5.2.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "i18n"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "repository": "https://github.com/vanska/gatsby-plugin-transform-i18n-locales",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
